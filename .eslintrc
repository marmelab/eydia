{
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "plugins": ["prettier", "jest"],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "es6": true,
        "node": true,
        "jest/globals": true
    },
    "rules": {
        "prettier/prettier": "error",
        "no-console": "warn"
    }
}